{
  "name": "yoru",
  "displayName": "Yoru",
  "description": "A dark theme for VS Code",
  "icon": "Yoru_icon.png",
  "version": "0.1.6",
  "publisher": "KahKitZheng",
  "repository": {
    "type": "git",
    "url": "https://github.com/KahKitZheng/yoru"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Themes"
